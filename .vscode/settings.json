{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true,
    "**/.cache": true,
    "**/.gradle": true,
    "**/.wpilib": true,
    "**/external": true,
    "**/gradle": true,
    "**/build": true,
    "**/compile_commands.json": true,
    "**/gradlew*": true,
    "**/vendordeps": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "iostream": "cpp"
  },
  "python.formatting.provider": "black"
}
